{
  "name": "instagram-commenter",
  "version": "0.0.10",
  "description": "An API library for building Instagram comment bots.",
  "bin": "lib/cli.js",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "pkg": {
    "assets": "assets/**/*"
  },
  "scripts": {
    "build": "npx tsc -p tsconfig.json",
    "build:watch": "npx tsc -w -p tsconfig.json",
    "build:compile": "npm run build && npx pkg package.json --out-path bin",
    "prepare": "npm run build",
    "test": "npm run build && npx jest --env=node",
    "start": "node lib/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chewyiscrunchy/instagram-commenter.git"
  },
  "author": "Braden Lamb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chewyiscrunchy/instagram-commenter/issues"
  },
  "homepage": "https://github.com/chewyiscrunchy/instagram-commenter#readme",
  "dependencies": {
    "commander": "^6.2.0",
    "inquirer": "^7.3.3",
    "instagram-connect": "0.0.13",
    "js-yaml": "^3.14.0",
    "tslog": "^3.0.1"
  },
  "devDependencies": {
    "@types/inquirer": "^7.3.1",
    "@types/js-yaml": "^3.12.5",
    "@types/node": "^14.14.10",
    "jest": "^26.6.3",
    "typescript": "^4.1.2"
  }
}
