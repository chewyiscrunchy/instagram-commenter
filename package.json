{
  "name": "instagram-commenter",
  "version": "0.0.1",
  "description": "An API library for building Instagram comment bots.",
  "bin": "lib/cli.js",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npx tsc -p tsconfig.json",
    "build:watch": "npx tsc -w -p tsconfig.json",
    "build:test": "npm run build && npm run test",
    "build:compile": "npm run build && npx pkg lib/cli.js --out-path bin",
    "test": "npx jest --env=node",
    "start": "node lib/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chewyiscrunchy/instagram-commenter.git"
  },
  "author": "Braden Lamb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chewyiscrunchy/instagram-commenter/issues"
  },
  "homepage": "https://github.com/chewyiscrunchy/instagram-commenter#readme",
  "dependencies": {
    "commander": "^6.2.0",
    "inquirer": "^7.3.3",
    "instagram-private-api": "^1.43.3",
    "instagram-web-api": "^2.2.2",
    "js-yaml": "^3.14.0"
  },
  "devDependencies": {
    "@types/inquirer": "^7.3.1",
    "@types/js-yaml": "^3.12.5",
    "@types/luxon": "^1.25.0",
    "@types/node": "^14.14.10",
    "@types/tough-cookie": "^4.0.0",
    "jest": "^26.6.3",
    "typescript": "^4.1.2"
  }
}
